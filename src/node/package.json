{
  "name": "@w-kuipers/alphadb",
  "version": "indev",
  "description": "Run AlphaDB in NodeJS",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
	"pywrapper_win32_x86_64",
	"pywrapper_linux_x86_64",
	"pywrapper_darwin_x86_64"
  ],
  "tsup": {
	"entry": ["index.ts"],
	"format": ["cjs", "esm"],
	"dts": true,
	"splitting": false,
	"sourcemap": true,
	"clean": true
  },
  "scripts": {
    "build": "tsup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/w-kuipers/alphadb.git"
  },
  "keywords": [
    "mysql"
  ],
  "author": "Wibo Kuipers",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/w-kuipers/alphadb/issues"
  },
  "homepage": "https://github.com/w-kuipers/alphadb#readme",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
